<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.qsxh.dao.IAdminDao">

    <!--管理员登录-->
    <select id="userLogin" resultType="com.qsxh.entity.Users">
            select userid, upass,uname from tbluser where userid = #{userid} and upass = #{upass}
    </select>



</mapper>
