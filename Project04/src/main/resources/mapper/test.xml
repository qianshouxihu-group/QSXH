<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.qsxh.dao.TestDao">

    <select id="userLogin" resultType="com.qsxh.entity.TblUser">
        select userid, upass,uname from tbluser where userid = #{userid} and upass = #{upass}
    </select>

</mapper>
